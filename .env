## Spring Boot runtime
SPRING_PROFILES_ACTIVE=dev

## AWS credentials (fill in or switch to AWS_PROFILE)
AWS_PROFILE=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=   # optional unless you use temporary creds (STS)
AWS_REGION=
AWS_ACCOUNT_ID=

## CDK / deployment defaults
CDK_DEFAULT_ACCOUNT=
CDK_DEFAULT_REGION=
STACK_PURPOSE=regression
ECR_REPOSITORY=core-spring-service
IMAGE_TAG=latest
ECR_IMAGE_URI=   # leave blank to derive from account/region/repo/tag
SERVICE_DESIRED_COUNT=1
